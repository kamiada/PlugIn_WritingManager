{

    "description": "Allows user to track progress in their writing projects",
    "manifest_version": 2,
    "name": "WritingManager",
    "version": "1.0",
    "homepage_url": "https://github.com/kamiada/PlugIn_WritingManager",
    "icons": {
      "48": "icons/beasts-48.png"
    },
  
    "permissions": [
      "activeTab"
    ],
  
    "browser_action": {
      "default_icon": "icons/beasts-32.png",
      "theme_icons": [{
          "light": "icons/beasts-32-light.png",
          "dark": "icons/beasts-32.png",
          "size": 32
      }],
      "default_title": "WritingManager",
      "default_popup": "popup/choose_beast.html"
    },
  
    "web_accessible_resources": [
      "beasts/*.jpg"
    ]
  
  }