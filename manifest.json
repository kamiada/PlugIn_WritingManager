{

    "description": "Allows user to track progress in their writing projects",
    "manifest_version": 2,
    "name": "WritingManager",
    "version": "1.0",
    "homepage_url": "https://github.com/kamiada/PlugIn_WritingManager",
    "background": {
        "scripts": ["popup/main.js"]
      },
    "permissions": [
      "activeTab"
    ],
  
    "browser_action": {
      "default_icon": "icons/softIcon.png",
      "default_title": "Writing Manager",
      "default_popup": "popup/main.html"
    }
  
  }